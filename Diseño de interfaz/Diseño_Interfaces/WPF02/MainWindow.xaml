<Window x:Class="WPF02.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WPF02"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <ComboBox x:Name="BoxPersonas" HorizontalAlignment="Left" Margin="83,97,0,0" VerticalAlignment="Top" Width="667" Height="35" SelectionChanged="ComboBox_SelectionChanged"/>
        <Label x:Name="Nombre" Content="Label" HorizontalAlignment="Left" Margin="109,217,0,0" VerticalAlignment="Top" Height="112" Width="192" Background="{x:Null}" HorizontalContentAlignment="Center" VerticalContentAlignment="Center"/>
        <Label x:Name="Apellidos" Content="Label" HorizontalAlignment="Left" Margin="468,217,0,0" VerticalAlignment="Top" Height="100" Width="188" FontFamily="Calibri" Cursor="Help" HorizontalContentAlignment="Center" VerticalContentAlignment="Center"/>

    </Grid>
</Window>
