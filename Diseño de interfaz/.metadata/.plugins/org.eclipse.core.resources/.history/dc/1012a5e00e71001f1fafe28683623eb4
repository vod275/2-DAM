
public class PilaDinamica<T> {
	
	private Nodo<T> top;
	private int tamanio;
	

	public PilaDinamica() {
		top = null;
		this.tamanio = 0;
		
	}
	
	
	public boolean isEmpty() {
		
		return top == null;
		
	}
	
	public int size() {
		return this.tamanio;
	}
	
	public T top() {
		if(isEmpty()) {
			return null;
		}
		else {
			return top.getElemento();
		}
			
	}
		
}
